{
  "name": "hwdtech-card",
  "version": "0.0.1",
  "private": true,
  "description": "Portfolio for HWdTech team",
  "scripts": {
    "start": "node ./dev-server.js",
    "lint": "eslint ./assets",
    "clean": "rimraf ./dist",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "build": "npm run clean; webpack --progress; cp manifest.json ./dist/manifest.json",
    "analyze": "webpack --profile --json > browser-build-stats.json",
    "deploy": "npm run precommit && npm run clean && npm run build && gh-pages -d dist -m 'Update gh-pages'"
  },
  "engines": {
    "node": ">=6.3.0",
    "npm": ">=3.10.3"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel": "6.5.2",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.9.0",
    "bemify": "0.1.2",
    "bourbon": "4.2.7",
    "breakpoint-sass": "git+ssh://git@github.com/at-import/breakpoint.git#2.x.x",
    "compression-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.0.1",
    "eslint-plugin-import": "1.10.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "handlebars-webpack-plugin": "0.1.0",
    "html-webpack-plugin": "2.22.0",
    "husky": "0.11.4",
    "lodash": "4.13.1",
    "moment": "2.14.1",
    "node-sass": "3.8.0",
    "offline-plugin": "3.4.2",
    "postcss-loader": "0.9.1",
    "pug": "2.0.0-beta5",
    "pug-loader": "2.2.1",
    "require-all": "2.0.0",
    "rimraf": "2.5.3",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "animate.css": "git+ssh://git@github.com/daneden/animate.css.git",
    "core-decorators": "0.12.3",
    "countup.js": "1.7.1",
    "raf": "3.2.0",
    "smooth-scroll": "github:cferdinandi/smooth-scroll",
    "waypoints": "4.0.0"
  }
}
